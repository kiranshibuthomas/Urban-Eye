{
  "name": "urbaneye-mern",
  "version": "1.0.0",
  "description": "UrbanEye - Smart City Complaint Management System (MERN Stack)",
  "main": "server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-server": "cd server && npm install",
    "install-client": "cd client && npm install",
    "install-all": "npm run install-server && npm run install-client",
    "start": "cd server && npm start",
    "heroku-postbuild": "npm run install-all && npm run build"
  },
  "keywords": [
    "mern",
    "react",
    "nodejs",
    "mongodb",
    "express",
    "authentication",
    "oauth",
    "tailwind"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "mongodb": "^6.18.0",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.6",
    "cookie-parser": "^1.4.6",
    "connect-mongo": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.2.1",
    "uuid": "^9.0.1"
  }
}
